<%= form_for(@frecuente) do |f| %>
  <% if @frecuente.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@frecuente.errors.count, "error") %> prohibited this frecuente from being saved:</h2>

      <ul>
      <% @frecuente.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :cliente_id %><br>
    <%= f.select("cliente_id",Cliente.all.collect{|l| [l.nombre, l.id ]}, {include_blank:'select Cliente'})%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
